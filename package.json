{
  "name": "cvs-svc-test-number",
  "version": "0.0.1",
  "description": "A microservice generating test numbers.",
  "main": "handler.js",
  "scripts": {
    "start": "node_modules/serverless/bin/serverless offline start",
    "build": "node_modules/typescript/bin/tsc --rootDir ./ --outDir .build --sourceMap false && npm run build:copy",
    "build:dev": "node_modules/typescript/bin/tsc && npm run build:copy",
    "build:copy": "find src -type f \\( -name \"*.yml\" -or -name \"*.json\" \\) | cpio -pdm .build && find tests -type f \\( -name \"*.yml\" -or -name \"*.json\" \\)  | cpio -pdm .build",
    "test": "node_modules/.bin/mocha .build/tests/**/*.unitTest.js",
    "test-i": "node_modules/.bin/mocha .build/tests/**/*.intTest.js",
    "lint": "tslint src/**/*.ts tests/**/*.ts",
    "sonar-scanner": "echo 'TODO'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.392.0",
    "joi": "^14.3.1",
    "node-yaml": "^3.1.1",
    "path-parser": "^4.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/chai": "^4.1.7",
    "@types/joi": "^14.3.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "gulp": "^4.0.0",
    "mocha": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "serverless": "^1.36.3",
    "serverless-dynamodb-local": "0.2.35",
    "serverless-offline": "^4.2.0",
    "serverless-plugin-typescript": "^1.1.5",
    "supertest": "^3.4.2",
    "swagger-jsdoc": "^3.2.6",
    "tslint": "^5.12.1",
    "tslint-jsdoc-rules": "^0.1.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.4"
  }
}
